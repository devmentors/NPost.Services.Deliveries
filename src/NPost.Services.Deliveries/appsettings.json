{
  "app": {
    "name": "NPost Deliveries Service"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "deliveries-service",
    "address": "localhost",
    "port": "5001",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "deliveries-service"
  },
  "httpClient": {
    "type": "",
    "retries": 2,
    "services": {
      "parcels": "parcels-service",
      "routing": "routing-service"
    }
  },
  "logger": {
    "applicationName": "deliveries-service",
    "excludePaths": ["/ping", "/metrics"],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "deliveries-service",
    "seed": false
  },
  "rabbitMq": {
    "namespace": "deliveries",
    "retries": 3,
    "retryInterval": 2,
    "messageProcessor": {
      "enabled": true,
      "type": "redis",
      "messageExpirySeconds": 300
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "deliveries:"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  }
}
